{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/global": true
  },
  "rules": {
    "linebreak-style": 0,
    "semi":"off",
    "arrow-parens":"off",
    "comma-dangle":"off",
    "max-len":"off",
    "require-jsdoc": "off"
  },
  "plugins": ["jest"],
  "extends": [
    "eslint:recommended", 
    "google"
  ]
}